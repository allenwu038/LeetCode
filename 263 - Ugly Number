# Python3
# runtime: faster than 82.30% of submissions
# memory: less than 6.67% of submissions
# NOTE: all numbers are beautiful :(

class Solution:
    def isUgly(self, num: int) -> bool:
        if num < 1:
            return False
        factors = [2, 3, 5]
        // divide num by each of our prime factors as many times as possible
        for i in factors:
            while num%i == 0:
                num = num//i   
        if num == 1:
            return True;
        return False
